<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="70dp"
            android:gravity="center"
            android:textSize="35dp"
            android:textColor="#000000"
            android:padding="2dp"
            android:background="@drawable/roundborder"

            android:text="@string/mypage" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"

                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#F1F1F1"
                        android:gravity="center"
                        android:text="@string/welcome" />

                    <TextView
                        android:id="@+id/myinfowelcome"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#F1F1F1"
                        android:gravity="center"
                        android:text="TextView"
                        android:textColor="#000000" />

                    <TextView
                        android:id="@+id/myemail"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#F1F1F1"
                        android:gravity="center"
                        android:text="TextView" />
                </LinearLayout>

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    android:layout_weight="0.2" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/partnerinfo"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:layout_weight="6"
                            android:text="TextView"
                            android:textSize="20dp"
                            android:textColor="#000000"/>

                        <ImageButton
                            android:id="@+id/openpartnerinfo"
                            style="@style/Widget.AppCompat.Button.Borderless"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:layout_weight="1"
                            android:scaleType="fitXY"
                            android:src="@drawable/ic_open"
                            tools:ignore="SpeakableTextPresentCheck" />
                        <ImageButton
                            android:id="@+id/closepartnerinfo"
                            style="@style/Widget.AppCompat.Button.Borderless"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:layout_weight="1"
                            android:scaleType="fitXY"
                            android:src="@drawable/ic_close"
                            tools:ignore="SpeakableTextPresentCheck"
                            android:visibility="gone"/>




                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/partnerlistlayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#C9F4A4"
                            android:text="@string/meal"
                            android:textSize="17dp"/>

                        <GridView
                            android:id="@+id/partnerlist1"
                            android:layout_width="match_parent"
                            android:layout_height="120dp"
                            android:numColumns="4"
                            android:scrollbars="horizontal"
                            android:horizontalSpacing="10dp"/>

                        <Space
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:layout_weight="0.2" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#6FF3FF"
                            android:text="@string/drink"
                            android:textSize="17dp"/>

                        <GridView
                            android:id="@+id/partnerlist2"
                            android:layout_width="match_parent"
                            android:layout_height="120dp"
                            android:scrollbars = "horizontal"
                            android:horizontalSpacing="10dp"
                            android:numColumns="4"/>
                    </LinearLayout>

                </LinearLayout>

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    android:layout_weight="0.2" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/cookinfo"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:layout_weight="6"
                            android:text="TextView"
                            android:textSize="20dp"
                            android:textColor="#000000"/>

                        <ImageButton
                            android:id="@+id/opencookinfo"
                            style="@style/Widget.AppCompat.Button.Borderless"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:layout_weight="1"
                            android:scaleType="centerCrop"
                            android:src="@drawable/ic_open"
                            tools:ignore="SpeakableTextPresentCheck" />
                        <ImageButton
                            android:id="@+id/closecookinfo"
                            style="@style/Widget.AppCompat.Button.Borderless"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:layout_weight="1"
                            android:scaleType="fitXY"
                            android:src="@drawable/ic_close"
                            tools:ignore="SpeakableTextPresentCheck"
                            android:visibility="gone"/>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/cookfoodlistlayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#C9F4A4"
                            android:text="@string/meal"
                            android:textSize="17dp"/>

                        <GridView
                            android:id="@+id/cookfoodlist1"
                            android:layout_width="match_parent"
                            android:layout_height="120dp"
                            android:numColumns="4"
                            android:scrollbars="horizontal"
                            android:horizontalSpacing="10dp"/>

                        <Space
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:layout_weight="0.2" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#6FF3FF"
                            android:text="@string/drink"
                            android:textSize="17dp"/>

                        <GridView
                            android:id="@+id/cookfoodlist2"
                            android:layout_width="match_parent"
                            android:layout_height="120dp"
                            android:scrollbars = "horizontal"
                            android:horizontalSpacing="10dp"
                            android:numColumns="4"/>
                    </LinearLayout>

                </LinearLayout>

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    android:layout_weight="0.2" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/outinfo"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:layout_weight="6"
                            android:text="TextView"
                            android:textSize="20dp"
                            android:textColor="#000000"/>

                        <ImageButton
                            android:id="@+id/openoutinfo"
                            style="@style/Widget.AppCompat.Button.Borderless"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:layout_weight="1"
                            android:scaleType="centerCrop"
                            android:src="@drawable/ic_open"
                            tools:ignore="SpeakableTextPresentCheck" />
                        <ImageButton
                            android:id="@+id/closeoutinfo"
                            style="@style/Widget.AppCompat.Button.Borderless"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:layout_weight="1"
                            android:scaleType="fitXY"
                            android:src="@drawable/ic_close"
                            tools:ignore="SpeakableTextPresentCheck"
                            android:visibility="gone"/>
                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/outfoodlistlayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#C9F4A4"
                            android:text="@string/meal"
                            android:textSize="17dp"/>




                                <GridView
                                    android:id="@+id/outfoodlist1"
                                    android:layout_width="match_parent"
                                    android:layout_height="120dp"
                                    android:horizontalSpacing="10dp"
                                    android:numColumns="4"
                                    />


                        <Space
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:layout_weight="0.2" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#6FF3FF"
                            android:text="@string/drink"
                            android:textSize="17dp"/>

                        <GridView
                            android:id="@+id/outfoodlist2"
                            android:layout_width="match_parent"
                            android:layout_height="120dp"
                            android:scrollbars = "horizontal"
                            android:numColumns="4"
                            android:horizontalSpacing="10dp"/>
                    </LinearLayout>


                </LinearLayout>

                <Space
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    android:layout_weight="0.2" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/deliveryinfo"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:layout_weight="6"
                            android:text="TextView"
                            android:textSize="20dp"
                            android:textColor="#000000"/>

                        <ImageButton
                            android:id="@+id/opendeliveryinfo"
                            style="@style/Widget.AppCompat.Button.Borderless"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:layout_weight="1"
                            android:scaleType="centerCrop"
                            android:src="@drawable/ic_open"
                            tools:ignore="SpeakableTextPresentCheck" />
                        <ImageButton
                            android:id="@+id/closedeliveryinfo"
                            style="@style/Widget.AppCompat.Button.Borderless"
                            android:layout_width="0px"
                            android:layout_height="wrap_content"
                            android:adjustViewBounds="true"
                            android:layout_weight="1"
                            android:scaleType="fitXY"
                            android:src="@drawable/ic_close"
                            tools:ignore="SpeakableTextPresentCheck"
                            android:visibility="gone"/>
                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/deliveryfoodlistlayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:orientation="vertical"
                        android:visibility="gone">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#C9F4A4"
                            android:textSize="17dp"
                            android:text="@string/meal" />

                        <GridView
                            android:id="@+id/deliveryfoodlist1"
                            android:layout_width="match_parent"
                            android:layout_height="120dp"
                            android:numColumns="4"
                            android:scrollbars="horizontal"
                            android:horizontalSpacing="10dp"/>

                        <Space
                            android:layout_width="match_parent"
                            android:layout_height="10dp"
                            android:layout_weight="0.2" />

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#6FF3FF"
                            android:text="@string/drink"
                            android:textSize="17dp"/>

                        <GridView
                            android:id="@+id/deliveryfoodlist2"
                            android:layout_width="match_parent"
                            android:layout_height="120dp"
                            android:scrollbars = "horizontal"
                            android:numColumns="4"
                            android:horizontalSpacing="10dp"/>
                    </LinearLayout>
                </LinearLayout>

            </LinearLayout>
        </ScrollView>
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>